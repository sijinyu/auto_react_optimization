{
  "name": "auto_react_optimization",
  "version": "1.0.0",
  "description": "React 코드베이스를 분석하여 성능 최적화를 제안하는 도구",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "react-auto-optimizer": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "analyze": "ts-node src/cli/index.ts analyze",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sijinyu/auto_react_optimization"
  },
  "keywords": [
    "react",
    "optimization",
    "memo",
    "useMemo",
    "useCallback"
  ],
  "author": "sijinyu <sijinyudev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sijinyu/auto_react_optimization/issues"
  },
  "homepage": "https://github.com/sijinyu/auto_react_optimization#readme",
  "dependencies": {
    "@babel/parser": "^7.22.5",
    "@babel/traverse": "^7.22.5",
    "@babel/types": "^7.22.5",
    "commander": "^9.4.1",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/generator": "^7.25.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/babel__traverse": "^7.20.6",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "packageManager": "yarn@4.5.0"
}
